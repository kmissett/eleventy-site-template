---
layout: "layouts/base.njk"
eleventyImport:
  collections: ["posts"]
pageID: "blog"
pageTitle: "Blog"
---

{% for post in collections.posts.reverse() %}
<article class="home-page-article">
	<h2 class="post-title"><a href="{{ post.url }}">{{ post.data.pageTitle }}</a></h2>
	<p class="post-excerpt">{{ post.data.excerpt }}</p>
</article>
{% endfor %}